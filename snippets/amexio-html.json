      "\t[http-url]=\"'${4:path}'\"",
      "\t[http-method]=\"'get'\"",
      "\t[enable-data-filter]=\"true\" (selectedRowData)=\"${5:onRowSelect}(\\$event)\">",
      "\t\t<amexio-data-table-column [data-index]=\"'${6:columnName}'\"",
