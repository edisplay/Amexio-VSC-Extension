      "\t\t[hidden]=\"false\"",
      "\t\t[text]=\"'${7:Name}'\">",
      "\t\t</amexio-data-table-column>",
      "</amexio-datagrid>"
    ],
    "description": "Amexio Data Grid Global Filter "
  },
  //Data-Groupby-Grid
  "amexio-data-groupby-grid": {
    "prefix": "amexio-data-groupby-grid",
    "body": [
      "<amexio-datagrid",
      "\t[title]=\"'${1:title}'\"",
      "\t[page-size] =\"${2:page-size}\"",
      "\t[data-reader]=\"'${3:data}'\"",
