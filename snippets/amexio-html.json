      "\t\t\t </ng-template>",
      "\t</amexio-data-table-column>",
      "</amexio-tree-data-table>"
    ],
    "description": "Amexio Tree Data Template"
  },
  //Tree
  //Tree Simple    
  "amexio-treeview": {
    "prefix": "amexio-treeview",
    "body": [
      "<amexio-treeview [data-reader]=\"'${1:data}'\"",
      "\t\t[data]=\"${2:treeLocalData}\"",
      "\t\t(nodeClick)=\"${3:getNodeData}(\\$event)\">",
      "</amexio-treeview>"
    ],
    "description": "Amexio  Data Treeview"
  },
  //Treeview templete
  "amexio-treeview-template": {
    "prefix": "amexio-treeview-template",
    "body": [
      "\t<amexio-treeview [data-reader]=\"'${1:data}'\"",
      "\t\t\t\t[http-url]=\"'${2:path}'\"",
      "\t\t\t\t[http-method]=\"'get'\"",
      "\t\t\t\t(nodeClick)=\"${3:getNodeData}(\\$event)\">",
      "\t\t<ng-template #amexioTreeTemplate \"${4:loop}\">",
      "\t\t\t ${5:Content}",
      "\t</ng-template>",
      "</amexio-treeview>"
    ],
    "description": "Amexio tree view with template"
  },
  //Tree Filter View
  "amexio-tree-filter-view": {
    "prefix": "amexio-tree-filter-view",
    "body": [
      "<amexio-tree-filter-view [data-reader]=\"'${1:data}'\"",
      "\t\t[http-method]=\"'get'\"",
      "\t\t\t\t(selectedRecord)=\"${2:getNodeData}(\\$event)\"",
      "\t\t[http-url]=\"'${3:path}'\">",
      "</amexio-tree-filter-view>"
    ],
    "description": "Amexio filter Tree View Data"
  },
  //Tree Checkbox
  "amexio-tree-checkbox": {
    "prefix": "amexio-tree-checkbox",
    "body": [
      "<amexio-treeview [enable-checkbox]=\"true\"",
      "\t\t[data-reader]=\"'${1:data}'\"",
      "\t\t[data]=\"${2:treeLocalData}\"",
      "\t (onTreeNodeChecked)=\"${3:getNodeData}(\\$event)\">",
      "</amexio-treeview>"
    ],
    "description": "Amexio tree checbox"
  },
   //Tree drag drop
   "amexio-tree-drag-drop": {
    "prefix": "amexio-tree-drag-drop",
    "body": [
      "<amexio-treeview [enable-drag]=\"true\"",
      "\t\t[enable-drop]=\"true\"",
      "\t\t [across-tree]=\"true\"",
      "\t\t[data-reader]=\"'${1:data}'\"",
      "\t\t[data]=\"${2:treeLocalData}\"",
      "\t (onTreeNodeChecked)=\"${3:getNodeData}(\\$event)\">",
      "</amexio-treeview>"
    ],
    "description": "Amexio tree drag drop"
  },
  //Tree with badge
  "amexio-tree-with-badge": {
    "prefix": "amexio-tree-with-badge",
    "body": [
      "<amexio-treeview [badge]=\"true\"",
      "\t\t[data-reader]=\"'${1:data}'\"",
      "\t\t[data]=\"${2:treeLocalData}\"",
      "\t (onTreeNodeChecked)=\"${3:getNodeData}(\\$event)\">",
      "</amexio-treeview>"
    ],
    "description": "Amexio tree with badge"
  },
  //Tree with context menu
  "amexio-tree-with-context-menu": {
    "prefix": "amexio-tree-with-context-menu",
    "body": [
      "<amexio-treeview [context-menu]=\"${4:data}\"",
      "\t\t[data-reader]=\"'${1:data}'\"",
      "\t\t[data]=\"${2:treeLocalData}\"",
      "\t (onTreeNodeChecked)=\"${3:getNodeData}(\\$event)\">",
      "</amexio-treeview>"
    ],
    "description": "Amexio tree with context-menu"
  },
  // Horizontal Tree View
  "amexio-treeview-horizontal": {
    "prefix": "amexio-tree-horizontal",
    "body": [
      "<amexio-horizontal-treeview [data-reader]=\"'${1:data}'\"",
      "\t[http-method]=\"'get'\"",
      "\t[label]=\"'${2:label}'\"",
      "\t(nodeClick)=\"${3:getNodeData}(\\$event)\"",
      "\t[http-url]=\"'${4:path}'\">",
      "</amexio-horizontal-treeview>"
    ],
    "description": "Amexio TreeView Horizontal"
  },
  //Drag and Drop
  "amexio-within-tree-drag-drop": {
    "prefix": "amexio-within-tree-drag-drop",
    "body": [
      "<amexio-treeview [data-reader]=\"'${1:data}'\"",
      "\t[enable-drag]=\"true\"",
      "\t[enable-drop]=\"true\"",
       "\t(nodeClick)=\"${3:getNodeData}(\\$event)\"",
      "\t[data]=\"'${4:data}'\">",
      "</amexio-treeview>"
    ],
    "description": "Amexio Within Tree Drag And Drop"
  },
  "amexio-across-tree-drag-drop": {
    "prefix": "amexio-across-tree-drag-drop",
    "body": [
      "<amexio-treeview #tree",
      "\t[data-reader]=\"'${1:data}'\"",
      "\t[enable-drag]=\"true\"",
      "\t[enable-drop]=\"true\"",
      "\t [across-tree]=\"true\"",
       "\t(nodeClick)=\"${3:getNodeData}(\\$event)\"",
      "\t[data]=\"'${4:data}'\">",
      "</amexio-treeview>"
    ],
    "description": "Amexio Across Tree Drag And Drop"
  },
  "amexio-tree-to-panel": {
    "prefix": "amexio-tree-to-panel",
    "body": [
      "<amexio-treeview [data-reader]=\"'${1:data}'\"",
      "\t[enable-drag]=\"true\"",
      "\t[enable-drop]=\"true\"",
      "\t [across-tree]=\"true\"",
       "\t(onDrag)=\"${3:drag}(\\$event)\"",
      "\t[data]=\"'${4:data}'\">",
      "</amexio--treeview>",
      "<amexio-panel (dragover)=\"${5:dragOver}(\\$event)\"",
      "\t (drop)=\"${6:drop}(\\$event)\"",
      "\t [header]=\"${7:true}\"",
      "\t [title]=\"'${8:title}'\"",
      "\t [expanded]=\"true\">",
      "\t\t\t ${9:content}",
      "</amexio-panel>"
    ],
    "description": "Amexio Across Tree Drag And Drop"
  },
  //Data Listbox
  "amexio-data-listbox": {
    "prefix": "amexio-data-listbox",
    "body": [
      "<amexio-listbox [enable-checkbox]=\"true\"",
      "\t\t\t[height]=\"${1:height}\"",
      "\t\t\t[header]=\"'${2:header}'\"",
      "\t\t\t[search-placeholder]=\"'${3:placeholder}'\"",
      "\t\t\t\t\t[data]=\"'${4:localData}'\"",
      "\t\t\t\t\t[filter]=\"true\"",
      "\t\t\t\t\t[data-reader]=\"'${5:response.data}'\"",
      "\t\t\t\t\t[display-field]=\"'${6:diaplayField}'\"",
      "\t\t\t\t\t(selectedRows)=\"'${8:getSelectedData}(\\$event)\">",
      "</amexio-listbox>"
    ],
    "description": "amexio data listbox"
  },
  //Data Listbox Template
  "amexio-listbox-template": {
    "prefix": "amexio-listbox-template",
    "body": [
      "<amexio-listbox [height]=\"'${1:height}'\"",
      "\t\t\t\t\t[header]=\"'${2:header}'\"",
      "\t\t\t\t\t[search-placeholder]=\"'${3:Search Placeholder}'\"",
      "\t\t\t\t\t[data]=\"${3:localData}\"",
      "\t\t\t\t\t[filter]=\"'false'\"",
      "\t\t\t\t\t[enable-checkbox]=\"'false'\"",
      "\t\t\t\t\t[data-reader]=\"'${4:response.data}'\"",
      "\t\t\t\t\t[display-field]=\"'${5:name}'\"",
      "\t\t\t\t\t(onRowClick)=\"'${6:onRowClick}(\\$event)'\">",
      "\t\t\t\t\t<ng-template #amexioBodyTmpl let-row=\"row\">",
      "\t\t\t\t\t\t ${7:Content}",
      "\t\t\t\t\t</ng-template>",
      "</amexio-listbox>"
    ],
    "description": "Amexio listbox template"
  }
}
