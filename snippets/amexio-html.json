  },
  //Grid-Tree-data-template
  "amexio-treedata-template": {
    "prefix": "amexio-treedata-template",
    "body": [
      "<amexio-tree-data-table",
      "\t[http-method]=\"'get'\"",
      "\t[http-url]=\"'${1:path}'\"",
      "\t[data-reader]=\"'${2:data}'\"",
      "\t(selectedRecord)=\"${3:getSelectedData}(\\$event)\">",
      "\t<amexio-data-table-column",
      "\t\t[data-index]=\"'${4:dataindex}'\"",
      "\t\t[data-type]=\"'string'\"",
      "\t\t[hidden]=\"false\"",
      "\t\t[text]=\"'{5:Name}'\">",
      "\t</amexio-data-table-column>",
      "\t<amexio-data-table-column ",
      "\t\t[data-index]=\"'${6:dataindex}'\"",
      "\t\t[data-type]=\"'number'\"",
      "\t\t[hidden]=\"false\"",
      "\t\t[text]=\"'${7:Name}'\">",
