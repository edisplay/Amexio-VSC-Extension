      "\t\t[hidden]=\"false\"",
      "\t\t[text]=\"'${7:Name}'\">",
      "\t\t</amexio-data-table-column>",
      "</amexio-datagrid>"
    ],
    "description": "Amexio Filter Grid Data "
  },
  // Data-Grid-Global-Filter
  "amexio-data-grid-filter": {
    "prefix": "amexio-data-grid-filter",
    "body": [
      "<amexio-datagrid",
      "\t[global-filter]=\"true\"",
      "\t[title]=\"'${1:title}'\"",
      "\t[page-size] = \"'${2:page-size}'\"",
