{ //Layout
  //Layout-accordian
  "amexio-accordion": {
    "prefix": "amexio-accordion",
    "body": [
      "<amexio-accordion angle-icon=\"${1:true}\">",
      "\t<amexio-accordion-tab ",
      "\t\theader=\"${2:header}\"",
      "\t\tactive=\"${3:true}\" ",
      "\t\tleft-icon=\"${4:icon}\"",
      "\t\tangle-icon=\"${5:false}\"",
      "\t\tdisabled=\"${6:false}\">",
        "\t\t\t${7:Content}",
        "\t</amexio-accordion-tab>",
      "</amexio-accordion>"
    ],
    "description": "Amexio Accordian Pane"
  },
  "amexio-accordion-tab": {
    "prefix": "amexio-accordion-tab",
    "body": [
      "\t<amexio-accordion-tab ",
      "\t\theader=\"${1:header}\"",
      "\t\tactive=\"${2:true}\" ",
      "\t\tleft-icon=\"${3:icon}\"",
      "\t\tangle-icon=\"${4:false}\"",
      "\t\tdisabled=\"${5:false}\">",
        "\t\t\t${6:Content}",
        "\t</amexio-accordion-tab>"
    ],
    "description": "Amexio Accordian Pane Tab"
  },
  "amexio-accordion-expand-all": {
    "prefix": "amexio-accordion-expand-all",
    "body": [
      "<amexio-accordion  expand-all=\"true\" angle-icon=\"${1:true}\">",
      "\t<amexio-accordion-tab ",
      "\t\theader=\"${4:header}\"",
      "\t\tactive=\"${9:true}\" ",
      "\t\tleft-icon=\"${5:icon}\"",
      "\t\tangle-icon=\"${6:false}\"",
      "\t\tdisabled=\"${7:false}\">",
        "\t\t\t${8:Content}",
        "\t</amexio-accordion-tab>",
      "</amexio-accordion>"
    ],
    "description": "Amexio Accordian Pane Expand All"
  },
  "amexio-accordion-transperent": {
    "prefix": "amexio-accordion-transperent",
    "body": [
      "<amexio-accordion transparent=\"true\" angle-icon=\"${1:true}\">",
      "\t<amexio-accordion-tab ",
      "\t\theader=\"${2:header}\"",
      "\t\tactive=\"${3:true}\" ",
      "\t\tleft-icon=\"${4:icon}\"",
      "\t\tangle-icon=\"${5:false}\"",
      "\t\tdisabled=\"${6:false}\">",
        "\t\t\t${7:Content}",
        "\t</amexio-accordion-tab>",
      "</amexio-accordion>"
    ],
    "description": "Amexio Accordian Pane Transperent"
  },
  //Layout-border
  "amexio-border-layout": {
    "prefix": "amexio-border-layout",
    "body": [
      "<amexio-borderlayout>",
      "\t<amexio-borderlayout-item position=\"${1:position}\">",
      "\t\t${2:Content}",
      "\t</amexio-borderlayout-item>",
      "</amexio-borderlayout>"
    ],
    "description": "Amexio Border Layout "
  },
  "amexio-border-layout-item": {
    "prefix": "amexio-border-layout-item",
    "body": [
      "<amexio-borderlayout-item position=\"${1:position}\">",
      "\t${2:Content}",
      "</amexio-borderlayout-item>"
    ],
    "description": "Amexio Border Layout Item"
  },
  //Layout-column
  "amexio-row": {
    "prefix": "amexio-row",
    "body": [
      "<amexio-row>",
      "\t",
      "</amexio-row>"
    ],
    "description": "Amexio Row"
  },
  "amexio-column": {
    "prefix": "amexio-column",
    "body": [
      "<amexio-column [size] =${1:size} >",
      "\t",
      "</amexio-column>"
    ],
    "description": "Amexio Column [size] :: 2:4:6:12"
  },
  //box
  "amexio-box": {
    "prefix": "amexio-box",
    "body": [
      "<amexio-box border-color =\"${1:border-color}\"",
      "\t border=\"${2:borderPosition}\"",
      "\t border-dotted=\"${3:false}\"",
      "\t padding=\"true\"",
      "closable=\"${8:false}\"",
      "\t align=\"${4:left}\">",
      "\t\t ${5:Content}",
      "</amexio-box>"
    ],
    "description": "Amexio Box "
  },
  "amexio-box-background": {
    "prefix": "amexio-box-background",
    "body": [
      "<amexio-box border-color =\"${1:border-color}\"",
      "\t border=\"${2:borderPosition}\"",
      "\t border-dotted=\"${3:false}\"",
      "\t background-color=\"${4:background-color}\"",
      "\t align=\"${5:left}\"",
      "closable=\"${6:false}\"",
      "\t padding=\"true\">",
      "\t\t ${7:Content}",
      "</amexio-box>"
    ],
    "description": "Amexio Box Background"
  },
  "amexio-box-background-height-width": {
    "prefix": "amexio-box-background-height-width",
    "body": [
      "<amexio-box border-color =\"${1:border-color}\"",
      "\t border=\"${2:borderPosition}\"",
      "\t border-dotted=\"${3:false}\"",
      "\t background-color=\"${4:background-color}\"",
      "\t box-height=\"${5:height}\"",
      "\t box-width=\"${6:width}\"",
      "\t align=\"${7:left}\"",
      "closable=\"${8:false}\"",
      "\t padding=\"true\">",
      "\t\t ${9:Content}",
      "</amexio-box>"
    ],
    "description": "Amexio Box with background height and width "
  },
  //Layout-card
  "amexio-card": {
    "prefix": "amexio-card",
    "body": [                                                                                                                                                                                                                         
      "<amexio-card [header]=\"true\"",
      "[header-align]=\"'right'\"",
      "[footer]=\"true\"",
      "[footer-align]=\"'right'\">",
      "\t<amexio-header>",
      "\t\t${3:header}",
      "\t</amexio-header>",
      "\t<amexio-body>",
      "\t\t${4:body}",
      "\t</amexio-body>",
      "\t<amexio-action>",
      "\t\t${5:footer}",
      "\t</amexio-action>",
      "</amexio-card>"
    ],
    "description": "Amexio Card "
  },
  "amexio-card-with-height": {
    "prefix": "amexio-card-with-height",
    "body": [
      "<amexio-card [header]=\"false\"",
      "[header-align]=\"'right'\"",
      "[footer]=\"false\"",
      "[footer-align]=\"'right'\"",  
      "min-height=\"${1:height}\">",          
      "height=\"${2:height}\">",
      "\t<amexio-body>",
      "\t${3:body}",
      "\t</amexio-body>",
      "</amexio-card>"
    ],
    "description": "Amexio Card with Height"
  },
  "amexio-card-with-flag": {
    "prefix": "amexio-card-with-flag",
    "body": [
      "<amexio-card [header]=\"false\"",
      "[header-align]=\"'right'\"",
      "[footer]=\"true\"",
      "[footer-align]=\"'right'\"",
      "[show]=\"${1:flag}\"",
      "[footer-align]=\"'center'\"",
      "height=\"${2:height}\">",
      "\t<amexio-body>",
      "\t${3:body}",
      "\t</amexio-body>",
      "</amexio-card>"
    ],
    "description": "Amexio Card with Flag"
  },
  "amexio-card-image": {
    "prefix": "amexio-card-image",
    "body": [
      "<amexio-card [header]=\"true\"",
      "[header-align]=\"'right'\"",
      "[footer]=\"false\"",
      "[footer-align]=\"'center'\">",
      "\t<amexio-header>",
      "\t\t${2:header}",
      "\t</amexio-header>",
      "\t<amexio-body>",
      "\t\t<amexio-image path=\"${3:image}\">",
      "\t\t</amexio-image> ",
      "\t</amexio-body>",
      "</amexio-card>"
    ],
    "description": "Amexio Card with image"
  },
  //Panel
  //Panel-carousel
  "amexio-carousel": {
    "prefix": "amexio-carousel",
    "body": [
      "<amexio-carousel [header]=\"'${1:Header}'\"",
      "\t\t[data]=\"${2:data}\">",
      "\t\t\t <ng-template let-img amexioTemplate=\"${3:item}\">",
      "\t\t\t\t ${4:content}",
      "\t\t\t </ng-template>",
      " </amexio-carousel>"
    ],
    "description": "Amexio Carousel"
  },
  "amexio-carousel-shuffle": {
    "prefix": "amexio-carousel-shuffle",
    "body": [
      "<amexio-carousel [header]=\"'${1:header}'\"",
      "\t\t[data]=\"${2:data}\"",
      "\t\t shuffle-interval=\"${3:interval}\">",
      "\t\t\t <ng-template let-img amexioTemplate=\"${4:item}\">",
      "\t\t\t\t ${5:content}",
      "\t\t\t </ng-template>",
      "</amexio-carousel>"
    ],
    "description": "Amexio Carousel with Shuffle Interval"
  },
//Panels-Form
"amexio-form": {
  "prefix": "amexio-form",
  "body": [                                                                                                                                                                                                                         
    "<amexio-form [header]=\"true\"",
    "\t [header-align]=\"'right'\"",
    "\t [footer]=\"true\"",
    "\t [footer-align]=\"'right'\"",
    "\t [min-height]=\"${1:height}\"",
    "\t [body-height]=\"'${2:height}'\"",
    "\t [form-name]=\"${3:name}\"",
    "\t [show-error]=\"${4:false}\">",
    "\t\t<amexio-form-header>",
    "\t\t\t${5:header}",
    "\t\t</amexio-form-header>",
    "\t\t<amexio-form-body>",
    "\t\t\t${6:body}",
    "\t\t</amexio-form-body>",
    "\t\t<amexio-form-action>",
    "\t\t\t${7:footer}",
    "\t\t</amexio-form-action>",
    "</amexio-form>"
  ],
  "description": "Amexio Form "
},
  //Panels-fieldset
  "amexio-fieldset": {
    "prefix": "amexio-fieldset",
    "body": [
      "<amexio-fieldset [collapsible]=\"true\"",
      "\t title=\"${1:title}\">",
      "</amexio-fieldset>"
    ],
    "description": "Amexio Fieldset"
  },
  "amexio-fieldset-collapsed": {
    "prefix": "amexio-fieldset-collapsed",
    "body": [
      "<amexio-fieldset [collapsible]=\"false\"",
      "\t title=\"${1:title}\">",
      "</amexio-fieldset>"
    ],
    "description": "Amexio Fieldset "
  },
  //Panels-panel
  "amexio-panel": {
    "prefix": "amexio-panel",
    "body": [
      "<amexio-panel [header]=\"true\"",
      "\t[title]=\"'${1:title}'\"",
      "\t[height]=\"${2:height}\"",
      "\t[expanded]=\"true\">",
      "",
      "</amexio-panel>"
    ],
    "description": "Amexio Panel"
  },
  "amexio-panel-custom-header": {
    "prefix": "amexio-panel-custom-header",
    "body": [
      "<amexio-panel [header]=\"true\"",
      "\t[title]=\"'${1:title}'\"",
      "\t[height]=\"${2:height}\"",
      "\t[expanded]=\"true\">",
      "\t\t <amexio-header>",
      "\t\t\t ${3:content}",
      "\t\t </amexio-header>",
      "</amexio-panel>"
    ],
    "description": "Amexio Panel Custom Header"
  },
  "amexio-panel-no-header": {
    "prefix": "amexio-panel-no-header",
    "body": [
      "<amexio-panel [header]=\"false\"",
      "\t[title]=\"'${1:title}'\"",
      "\t[height]=\"${2:height}\"",
      "\t[expanded]=\"true\">",
      "</amexio-panel>"
    ],
    "description": "Amexio Panel without Header"
  },
  //Panels-steps
  "amexio-steps": {
    "prefix": "amexio-steps",
    "body": [
      "<amexio-steps [block]=\"true\" [index]=\"true\">",
      "\t <amexio-step-block [label]=\"'${1:label}'\"",
      "\t [active]=\"true\" >",
      "\t </amexio-step-block>",
      "</amexio-steps>"
    ],
    "description": "Amexio Steps"
  },
  "amexio-steps-block": {
    "prefix": "amexio-steps-block",
    "body": [
      "\t <amexio-step-block [label]=\"'${1:label}'\"",
      "\t\t [active]=\"true\">",
      "\t </amexio-step-block>"
    ],
    "description": "Amexio Steps Block"
  },
  "amexio-steps-block-with-icon": {
    "prefix": "amexio-step-block-with-icon",
    "body": [
      "<amexio-steps ",
      "\t [block]=\"true\"",
      "\t [icon]=\"true\"",
      "\t (onClick)=\"${1:functionName}(\\$event)\\",
      "\t <amexio-step-block",
      "\t\t [label]=\"'${2:label}'\"",
      "\t\t [active]=\"true\"",
      "\t\t [icon]=\"${3:icon}\" >",
      "\t </amexio-step-block>",
      "</amexio-steps>"
    ],
    "description": "Amexio Steps Block with Icon"
  },
  "amexio-steps-clickable": {
    "prefix": "amexio-steps-clickable",
    "body": [
      "<amexio-steps [block]=\"true\"",
      "\t [index]=\"true\"",
      "\t (onClick)=\"${1:onClickEvent}(\\$event)\">",
      "\t\t <amexio-step-block [label]=\"'${2:label}'\"",
      "\t\t [active]=\"true\" >",
      "\t\t </amexio-step-block>",
      "</amexio-steps>"
    ],
    "description": "Amexio Steps Clickable"
  },
  "amexio-steps-with-index": {
    "prefix": "amexio-steps-with-index",
    "body": [
      "<amexio-steps ",
      "\t [index]=\"true\"",
      "\t (onClick)=\"${1:onClickEvent}(\\$event)\">",
      "\t\t <amexio-step-block [label]=\"'${2:label}'\"",
      "\t\t [active]=\"true\" >",
      "\t\t </amexio-step-block>",
      "</amexio-steps>"
    ],
    "description": "Amexio Steps with Index"
  },
  //Panels-tab
  //Panels-tab-horizontal
  "amexio-tab-view": {
    "prefix": "amexio-tab-view",
    "body": [
      "<amexio-tab-view  [closable]=\"false\">",
      "\t <amexio-tab title=\"${1:title}\"",
      "\t [active]=\"true\">",
      "\t\t${2:content}",
      "\t </amexio-tab>",
      "</amexio-tab-view>"
    ],
    "description": "Amexio Tab View"
  },
  "amexio-tab": {
    "prefix": "amexio-tab",
    "body": [
      "\t <amexio-tab title=\"${1:title}\"",
      "\t [active]=\"false\">",
      "\t\t${2:content}",
      "\t </amexio-tab>"
    ],
    "description": "Amexio Tab"
  },
  "amexio-tab-view-closable": {
    "prefix": "amexio-tab-view-closable",
    "body": [
      "<amexio-tab-view  [closable]=\"true\">",
      "\t <amexio-tab title=\"${1:title}\"",
      "\t [active]=\"true\">",
      "\t\t${2:content}",
      "</amexio-tab>",
      "</amexio-tab-view>"
    ],
    "description": "Amexio Closable Tab"
  },
  "amexio-tab-with-icon": {
    "prefix": "amexio-tab-with-icon",
    "body": [
      "<amexio-tab title=\"${1:title}\"",
      "\t [active]=\"false\"",
      "\t [icon]=\"'${2:icon}'\">",
      "\t\t${3:content}",
      "</amexio-tab>"
    ],
    "description": "Amexio Tab with Icon"
  },
  "amexio-tab-view-with-icon": {
    "prefix": "amexio-tab-with-icon",
    "body": [
      "<amexio-tab-view  [closable]=\"false\">",
      "\t<amexio-tab title=\"${1:title}\"",
      "\t [active]=\"true\"",
      "\t [icon]=\"'${2:icon}'\">",
      "\t\t${3:content}",
      "\t</amexio-tab>",
      "</amexio-tab-view>"
    ],
    "description": "Amexio Tab with Icon"
  },
  "amexio-enhanced-tab-view": {
    "prefix": "amexio-enhanced-tab-view",
    "body": [
      "<amexio-tab-view  [closable]=\"false\"",
      "\t [header]=\"${1:true}\"",
      "\t [header-align]=\"'${2:left}'\"",
      "\t [action]=\"${3:true}\"",
      "\t [divide-header-equally]=${4:true}",
      "\t [tab-position]=\"'${5:top}'\">",
      "\t\t <amexio-tab title=\"${6:title}\"",
      "\t\t [active]=\"true\"",
      "\t\t [icon]=\"'${7:icon}'\"",
      "\t\t [disabled]=\"${8:false}\">",
      "\t\t\t${9:content}",
      "\t\t </amexio-tab>",
      "</amexio-tab-view>"
    ],
    "description": "Amexio Enhanced Tab View"
  },
  //Panels-tab-Vertical
  "amexio-tab-view-vertical": {
    "prefix": "amexio-tab-view-vertical",
    "body": [
      "<amexio-vertical-tab-view>",
      "\t<amexio-tab title=\"${1:title}\"",
      "\t [disabled]=\"${2:false}\"",
      "\t [active]=\"true\"",
      "\t [icon]=\"'${3:icon}'\">",
      "\t\t${4:content}",
      "\t</amexio-tab>",
      "</amexio-vertical-tab-view>"
    ],
    "description": "Amexio Vertical Tab "
  },
  "amexio-tab-view-vertical-right": {
    "prefix": "amexio-tab-view-vertical-right",
    "body": [
      "<amexio-right-vertical-tab-view>",
      "\t<amexio-tab title=\"${1:title}\"",
      "\t [disabled]=\"${2:false}\"",
      "\t [active]=\"true\"",
      "\t [icon]=\"'${3:icon}'\">",
      "\t\t${4:content}",
      "\t</amexio-tab>",
      "<amexio-right-vertical-tab-view>"
    ],
    "description": "Amexio Right Vertical Tab "
  },
  //Panels-window
  "amexio-window": {
    "prefix": "amexio-window",
    "body": [
      "<amexio-window [show]=\"${1:showFlag}\"",
      "\t type=\"window\"",
      "\t [closable]=\"false\">",
      "\t [vertical-position]=\"'${5:center}'\"",
      "\t [horizontal-position]=\"'${6:center}'\"",
      "\t [close-on-escape]=\"${7:true}\"",
      "\t [material-design]=\"${8:true}\"",
      "\t [footer-align]=\"'${9:right}'\"",
      "\t\t<amexio-header>",
      "${2:header}",
      "\t\t</amexio-header>",
      "\t\t<amexio-body>",
      "${3:body}",
      "\t\t</amexio-body>",
      "\t\t<amexio-action>",
      "${4:action}",
      "\t\t</amexio-action>",
      "</amexio-window>"
    ],
    "description": "Amexio Window "
  },
  "amexio-window-maximize": {
    "prefix": "amexio-window-maximize",
    "body": [
      "<amexio-window [show-window]=\"${1:showFlag}\"",
      "\t type=\"window\"",
      "\t [vertical-position]=\"'${5:center}'\"",
      "\t [horizontal-position]=\"'${6:center}'\"",
      "\t [close-on-escape]=\"${7:true}\"",
      "\t [material-design]=\"${8:true}\"",
      "\t [footer-align]=\"'${9:right}'\"",
      "\t [closable]=\"false\"",
      "\t [maximize]=\"true\">",
      "\t\t<amexio-header>",
      "${2:header}",
      "\t\t</amexio-header>",
      "\t\t<amexio-body>",
      "${3:body}",
      "\t\t</amexio-body>",
      "\t\t<amexio-action>",
      "${4:action}",
      "\t\t</amexio-action>",
      "</amexio-window>"
    ],
    "description": "Amexio Window Maximized "
  },
  "amexio-window-closable": {
    "prefix": "amexio-window-closable",
    "body": [
      "<amexio-window [show-window]=\"${1:showFlag}\"",
      "\t type=\"window\"",
      "\t [closable]=\"true\"",
      "\t [vertical-position]=\"'${5:center}'\"",
      "\t [horizontal-position]=\"'${6:center}'\"",
      "\t [close-on-escape]=\"${7:true}\"",
      "\t [material-design]=\"${8:true}\"",
      "\t [footer-align]=\"'${9:right}'\"",
      "\t\t<amexio-header>",
      "${2:header}",
      "\t\t</amexio-header>",
      "\t\t<amexio-body>",
      "${3:body}",
      "\t\t</amexio-body>",
      "\t\t<amexio-action>",
      "${4:action}",
      "\t\t</amexio-action>",
      "</amexio-window>"
    ],
    "description": "Amexio Window Closable "
  },
  //Panels-dialogue
  "amexio-dialogue-confirm": {
    "prefix": "amexio-dialogue-confirm",
    "body": [
      "<amexio-dialogue [show]=\"${1:showFlag}\"",
      "\t [title]=\"'${2:title}'\"",
      "\t [message]=\"'${3:message}'\"",
      "\t [primary-action-label]=\"'ok'\"",
      "\t [secondary-action-label]=\"'ok'\"",
      "\t [footer-align]=\"'${4:right}'\"",
      "\t [button-size]=\"'${5:small}'\"",
      "\t [content-align]=\"'${6:center}'\"",
      "\t [close-on-escape]=\"${7:true}\"",
      "\t [material-design]=\"${8:true}\"",
      "\t [message-type]=\"'confirm'\"",
      "\t [type]=\"'confirm'\">",
      "</amexio-dialogue>"
    ],
    "description": "Amexio Confirmation Dialogue Box "
  },
  "amexio-dialogue-error": {
    "prefix": "amexio-dialogue-error",
    "body": [
      "<amexio-dialogue [show-window]=\"${1:showFlag}\"",
      "\t [title]=\"'${2:title}'\"",
      "\t [message]=\"'${3:message}'\"",
      "\t [primary-action-label]=\"'ok'\"",
      "\t [secondary-action-label]=\"'ok'\"",
      "\t [footer-align]=\"'${4:right}'\"",
      "\t [button-size]=\"'${5:small}'\"",
      "\t [content-align]=\"'${6:center}'\"",
      "\t [close-on-escape]=\"${7:true}\"",
      "\t [material-design]=\"${8:true}\"",
      "\t [message-type]=\"'error'\"",
      "\t [type]=\"'alert'\">",
      "</amexio-dialogue>"
    ],
    "description": "Amexio Error Dialogue Box "
  },
  "amexio-dialogue-warning": {
    "prefix": "amexio-dialogue-warning",
    "body": [
      "<amexio-dialogue [show-window]=\"${1:showFlag}\"",
      "\t [title]=\"'${2:title}'\"",
      "\t [message]=\"'${3:message}'\"",
      "\t [primary-action-label]=\"'ok'\"",
      "\t [secondary-action-label]=\"'ok'\"",
      "\t [footer-align]=\"'${4:right}'\"",
      "\t [button-size]=\"'${5:small}'\"",
      "\t [content-align]=\"'${6:center}'\"",
      "\t [close-on-escape]=\"${7:true}\"",
      "\t [material-design]=\"${8:true}\"",
      "\t [message-type]=\"'warning'\"",
      "\t [type]=\"'alert'\">",
