  },
  //Data-Grid-with-context-menu
  "amexio-data-grid-with-context-menu": {
    "prefix": "amexio-data-grid-with-context-menu",
    "body": [
      "<amexio-datagrid",
      "\t [context-menu]=\"${7:data}\"",
      "\t[enable-column-fiter]=\"true\" [height]=\"${1:height}\" title=\"${2:title}\"",
      "\t[enable-checkbox]=\"false\"",
      "\t[http-method]=\"'get'\"",
      "\t[http-url]=\"'${3:path}'\"",
      "\t[data-reader]=\"'${4:data}'\" ",
      "\t[page-size] = \"${5:page-size}\">",
      "\t<amexio-data-table-column [data-index]=\"'${6:dataindex}'\"",
      "\t\t[context-menu]=\"${8:data}\"",
      "\t\t[data-type]=\"'string'\"",
      "\t\t[hidden]=\"false\"",
      "\t\t\t[text]=\"'Name'\">",
      "\t</amexio-data-table-column>",
      "</amexio-datagrid"
    ],
    "description": "Amexio Data Grid With Context Menu"
