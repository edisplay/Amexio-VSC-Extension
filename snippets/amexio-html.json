    "body": [
      "<amexio-media-ee-carousel title=\"${1:title}\"",
      "\t[data]=\"${2:data}\">",
      "</amexio-media-ee-carousel>"
    ],
    "description": "Amexio Enterprise multi item carousel horizontal"
  },
  "amexio-enterprise-muiti-item-carousel-vertical": {
    "prefix": "amexio-enterprise-multi-item-carousel-vertical",
    "body": [
      "<amexio-media-ee-carousel title=\"${1:title}\"",
      "\tcarousel-type=\"vertical\"",
      "\t[data]=\"${2:data}\">",
      "</amexio-media-ee-carousel>"
    ],
    "description": "Amexio Enterprise multi item carousel vertical"
  },
  //Data
  //Data Item Selector
  "amexio-data-item-selector": {
    "prefix": "amexio-data-item-selector",
    "body": [
      "<amexio-item-selector [height]=\"'${1:height}'\"",
      "\t[display-field]=\"'${2:feild1}'\"",
      "\t[value-field]=\"'${3:feild2}'\"",
      "\t(selectedRecords)=\"${4:getSelectedData}(\\$event)\"",
      "\t[http-url]=\"'${5:path}'\"",
      "\t[http-method]=\"'get'\"",
      "\t[data-reader]=\"'${6:data}'\">",
      "</amexio-item-selector>"
    ],
    "description": "Amexio Item Data Selector"
  },
  //Data-Grid
  "amexio-data-grid-simple": {
    "prefix": "amexio-data-grid-simple",
    "body": [
      "<amexio-datagrid",
      "\t[enable-column-fiter]=\"true\"",
      "\ttitle=\"'${1:title}'\"",
      "\t[http-method]=\"'get'\"",
      "\t[http-url]=\"'${2:path}'\"",
      "\t[data-reader]=\"'${3:data}'\"",
      "\t[page-size] = \"${4:page-size}\"",
      "\t(selectedRowData)=\"${5:onSelectRow}(\\$event)\">",
      "\t<amexio-data-table-column [data-index]=\"'${6:columnName}'\"",
      "\t\t[data-type]=\"'string'\" [hidden]=\"false\"",
      "\t\t[text]=\"'${7:Name}'\">",
      "\t</amexio-data-table-column>",
      "</amexio-datagrid>"
    ],
    "description": "Amexio Simple Data Grid "
  },
  //Data-Grid-Column
  "amexio-data-grid-column": {
    "prefix": "amexio-data-grid-column",
    "body": [
      "<amexio-data-table-column [data-index]=\"'${6:columnName}'\"",
      "\t\t[data-type]=\"'string'\" [hidden]=\"false\"",
      "\t\t[text]=\"'${7:Name}'\">",
      "</amexio-data-table-column>"
    ],
    "description": "Amexio Data Grid Column "
  },
  //Data-Grid-Template
  "amexio-data-grid-template": {
    "prefix": "amexio-data-grid-template",
    "body": [
      "<amexio-datagrid  title=\"'${1:title}'\"",
      "\t[http-method]=\"'get'\"",
      "\t[http-url]=\"'${2:path}'\"",
      "\t[data-reader]=\"'${3:data}'\"",
      "\t[page-size] = \"${4:pagesize}\"",
      "\t[enable-data-filter]=\"false\"",
      "\t(rowSelect)=\"'${5:getRowData}(\\$event)'\">",
      "\t<amexio-data-table-column [width]=\"${6:width}\"",
      "\t\t[data-index]=\"'${7:dataIndex}'\"",
      "\t\t[data-type]=\"'string'\"",
      "\t\t[hidden]=\"false\"",
      "\t\t[text]=\"'${8:Name}'\">",
      "\t\t<ng-template #amexioBodyTmpl \"${9:loop}\">",
      "\t\t ${10:Content} ",
      "\t\t</ng-template>",
      "\t</amexio-data-table-column>",
      "</amexio-datagrid>"
    ],
    "description": "Amexio Data Grid Template"
  },
  //Data-Grid-Header-Template
  "amexio-data-grid-header-template": {
    "prefix": "amexio-data-grid-header-template",
    "body": [
      "<amexio-datagrid  title=\"'${1:title}'\"",
      "\t[http-method]=\"'get'\"",
      "\t[http-url]=\"'${2:path}'\"",
      "\t[data-reader]=\"'${3:data}'\"",
      "\t[page-size] = \"${4:pagesize}\"",
      "\t[enable-data-filter]=\"false\"",
      "\t(rowSelect)=\"'${5:getRowData}(\\$event)'\">",
      "\t<amexio-data-table-column [width]=\"${6:width}\"",
      "\t\t[data-index]=\"'${7:dataIndex}'\"",
      "\t\t[data-type]=\"'string'\"",
      "\t\t[hidden]=\"false\"",
      "\t\t[text]=\"'${8:Name}'\">",
      "\t\t<ng-template #amexioHeaderTmpl let-column=\"column\" let-index=\"index\">",
      "\t\t\t\t${9:content}",
       "\t\t</ng-template>",
      "\t</amexio-data-table-column>",
      "</amexio-datagrid>"
    ],
    "description": "Amexio Data Grid Header Template "
  },
  // Data-Grid-Filter
  "amexio-data-grid-filter": {
    "prefix": "amexio-data-grid-filter",
    "body": [
      "<amexio-datagrid",
      "\t[title]=\"'${1:title}'\"",
