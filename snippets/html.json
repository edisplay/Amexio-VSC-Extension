  //Button Group Snippets
  "amexio-button-group": {
    "prefix": "amexio-button-group",
    "body": [
      "<amexio-btn-group>",
      "\t <amexio-btn-group-action [label]=\"'${1:btnGroupLabel}'\">",
      "\t </amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${2:label1}'\"",
      "\t [type]=\"'Default'\"> \n</amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${3:label2}'\"",
      "\t [type]=\"'primary'\"> \n</amexio-btn-group-action> ",
      "\t <amexio-btn-group-action [label]=\"'${4:label3}'\"",
      "\t [type]=\"'success'\"> \n</amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${5:label3}'\"",
