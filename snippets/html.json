//FIELDSET
"amexio-fieldset":{
  "prefix": "amexio-fieldset",
  "body": ["<amexio-fieldset [collapsible]=\"true\" [title]=\"'${1:Title}'\">",
  "<amexio-fieldset-body>",
  "<div class=\"form-group row\">",
  "<div class=\"col-6\">",
  "<amexio-text-input [fieldLabel]=\"'${2:fieldLabel}'\" name =\"${3:name}\"",
                                  "[placeholder]=\"'${4:placeholder}'\"",
                                   "[iconFeedBack]=\"true\"></amexio-text-input>",
  " </div>",
    
  "<div class=\"col-6\">",
  "<amexio-text-input [fieldLabel]=\"'${2:fieldLabel}'\" name =\"${3:name}\"",
                                  "[placeholder]=\"'${4:placeholder}'\"",
                                   "[iconFeedBack]=\"true\"></amexio-text-input>",
  " </div>",
  
  "<div class=\"col-6\">",
  "<amexio-text-input [fieldLabel]=\"'${2:fieldLabel}'\" name =\"${3:name}\"",
                                  "[placeholder]=\"'${4:placeholder}'\"",
                                   "[iconFeedBack]=\"true\"></amexio-text-input>",
  " </div>",
    
  "<div class=\"col-6\">",
  "<amexio-email-input [fieldLabel]=\"'${2:fieldLabel}'\" name =\"${3:name}\"",
                                  "[placeholder]=\"'${4:placeholder}'\"",
                                  "[allowBlank]=\"false\" [errorMsg] =\"'${5:errorMsg}'\"",
                                   "[iconFeedBack]=\"true\"></amexio-email-input>",
  " </div>",

  "<div class=\"col-12\">",
  "<amexio-textarea-input [fieldLabel]=\"'${2:fieldLabel}'\" name =\"${3:name}\"",
                                  "[placeholder]=\"'${4:placeholder}'\"",
                                  "[errorMsg] =\"'${5:errorMsg}'\"",
                                  "[iconFeedBack]=\"true\" [noOfrows]=\"'${6:noOfrows}'\" [noOfCols]=\"'${7:noOfCols}'\"",
                                  "[popoverPlacement]=\"'${8:top}'\">",
