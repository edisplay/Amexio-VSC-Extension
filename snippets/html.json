      "\t\t [dataType]=\"'${14:dataType3}'\" [hidden]=\"false\"",
      "\t\t [text]=\"'${15:text}'\" [summaryCaption]=\"'${20:summaryCaption}'\"",
      "\t [summaryType]=\"'${16:summaryType}'\"></amexio-data-table-column>",
      "</amexio-data-table>"
    ],
