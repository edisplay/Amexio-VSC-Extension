  //CheckBox Group Snippet
  "amexio-check-box": {
    "prefix": "amexio-horizontal-check-box",
    "body": [
      "<amexio-checkbox [fieldLabel]=\"'Horizontal Checkbox'\" name =\"${1:name}\"",
      "\t [dataReader]=\"'response.data'\"",
      "\t [displayField]=\"'${2:displayField}'\"",
      "\t [valueField]=\"'${3:valueField}'\"",
      "\t [checkBoxGroupDownBindData]=\"checkboxGroupdata\"",
      "\t [column]=\"'${4:columnCount}'\">",
      "</amexio-checkbox>"
    ],
    "description": "Amexio Horizontal Checkbox Group"
  },
  "amexio-check-box-vertical": {
    "prefix": "amexio-vertical-check-box",
    "body": [
      "<amexio-checkbox [fieldLabel]=\"'Vertical Checkbox'\" name =\"'${1:name}'\"",
      "\t [dataReader]=\"'response.data'\"",
      "\t [displayField]=\"'${2:displayField}'\"",
      "\t [valueField]=\"'${3:valueField}'\"",
      "\t [checkBoxGroupDownBindData]=\"checkboxGroupdata\"",
      "\t [column]=\"'${4:columnCount}'\">",
      "</amexio-checkbox>"
    ],
    "description": "Amexio Vertical Checkbox Group"
  },
  //Radio Group Snippet
  "amexio-radio-group": {
    "prefix": "amexio-radio-group",
    "body": [
      " <amexio-radio-group [fieldLabel]=\"'Horizontal RadioGroup'\" name =\"${1:name}\"",
      "\t [fieldName]=\"'${2:fieldName}'\"",
      "\t [dataReader]=\"'${3:dataReader}'\"",
      "\t [displayField]=\"'${4:displayField}'\"",
      "\t [valueField]=\"'${5:valueField}'\"",
      "\t [radioGroupBindData]=\"radioGroupData\"",
      "\t (selectedValue)=\"setSelectedGender($${6:event})\"",
      "\t [column]=\"'${7:column}'\">",
      " </amexio-radio-group>"
    ],
    "description": "Amexio Horizontal Radio Group"
  },
  "amexio-radio-group-vertical": {
    "prefix": "amexio-radio-group-vertical",
    "body": [
      " <amexio-radio-group [fieldLabel]=\"'Vertical RadioGroup'\" name =\"${1:name}\"",
      "\t [fieldName]=\"'${2:fieldName}'\"",
      "\t [dataReader]=\"'${3:dataReader}'\"",
      "\t [displayField]=\"'${4:displayField}'\"",
      "\t [valueField]=\"'${5:valueField}'\"",
      "\t [radioGroupBindData]=\"radioGroupData\"",
      "\t (selectedValue)=\"setSelectedGender($${7:event})\"",
      "\t [column]=\"'${8:column}'\">",
      " </amexio-radio-group>"
    ],
    "description": "Amexio Vertical Radio Group"
  },
  //type ahead
  "amexio-text-type-ahead": {
    "prefix": "amexio-text-type-ahead",
    "body": [
      "<amexio-typeahead [dataReader]=\"'response.data'\"",
      "\t [httpUrl]=\"'${1:httpUrl}'\"",
      "\t [httpMethod]=\"'${2:httpMethod}'\"",
      "\t [key]=\"'${3:key}'\"",
      "\t [(ngModel)]=\"${4:ngModel}\"",
      "\t [fieldLabel]=\"'${5:fieldLabel}'\">",
      "</amexio-typeahead>"
    ],
    "description": "Amexio Text Type Ahead"
  },
