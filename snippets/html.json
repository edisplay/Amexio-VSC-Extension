  //Tab Pane
  "amexio-tab": {
    "prefix": "amexio-tab",
    "body": [
      "<amexio-tab-pane  [closable]=\"false\">",
      "\t <amexio-tab title=\"${1:title}\" [active]=\"true\"",
      "\t [icon]=\"'${2:icon}'\">",
      "\t<h4>${3:heading}</h4>",
      "\t${4:text}",
      "</amexio-tab>",
      "<amexio-tab title=\"${5:title1}\"  [icon]=\"'${6:icon1}'\">",
      "\t<h4>${7:heading1}</h4>",
      "\t${8:text1}",
      "</amexio-tab>",
      "<amexio-tab title=\"${9:title2}\" [icon]=\"'${10:icon3}'\">",
      "\t<h4>${11:heading4}</h4>",
      "\t${12:text5}",
      "</amexio-tab>",
      "</amexio-tab-pane>"
    ],
    "description": "Amexio Simple Tab Pane "
  },
