      " <amexio-item-selector [height]=\"'${1:height}'\"",
      "\t [displayField]=\"'${2:displayField}'\"",
      "\t [valueField]=\"'${3:valueField}'\"",
      "\t (selectedRecords)=\"getSelectedData($${4:event})\"",
