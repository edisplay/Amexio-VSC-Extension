      "export class ${3:Name}Component implements OnInit {",
      "\t${4:buttonLabel} : string = 'Stop';",
      "\t${5:progressStat} : boolean = true;",
      "\tconstructor() { }",
      "",
      "\tngOnInit() { }$0",
      "",
      "\tbuttonClick(event : any){",
      "\tif(this.${5:progressStat}){",
      "\t\tthis.${5:progressStat} = !this.${5:progressStat};",
      "\t\tthis.${4:buttonLabel} = 'Start';",
      "\t}",
      "\telse{",
      "\t\tthis.${5:progressStat} = !this.${5:progressStat};",
      "\t\tthis.${4:buttonLabel} = 'Stop';",
      "\t}",
      "}",
    "}"
    ]
  },

